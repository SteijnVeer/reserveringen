{
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "baseUrl": "..",
    "erasableSyntaxOnly": true,
    "jsx": "react-jsx",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "moduleDetection": "force",
    "moduleResolution": "bundler",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedSideEffectImports": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "paths": {
      "@components/*": [
        "commons/components/*"
      ],
      "@constants/*": [
        "commons/constants/*"
      ],
      "@contexts/*": [
        "commons/contexts/*"
      ],
      "@hooks/*": [
        "commons/hooks/*"
      ],
      "@package/*": [
        "package.json"
      ],
      "@types/*": [
        "commons/types/*"
      ],
      "@utils/*": [
        "commons/utils/*"
      ]
    },
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2022",
    "tsBuildInfoFile": ".tmp/tsconfig.app.tsbuildinfo",
    "types": [
      "vite/client"
    ],
    "useDefineForClassFields": true,
    "verbatimModuleSyntax": true
  }
}